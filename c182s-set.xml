      <!-- model defined later in the nasal section of this file. -->
      <path></path>
    </electrical>
  </systems>
  
  
<instrumentation n="0">
    <path>Aircraft/c182/Systems/instrumentation.xml</path>
  </instrumentation>

  <sound>
