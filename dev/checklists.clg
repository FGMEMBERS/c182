item("Ignition Switch") {
    s = "controls/switches/starter";
    m = "/controls/engines/engine[0]/magnetos";
    state("START", s == 1) {
        s = 1;
        m = 3;
    }
    state("OFF", s==0&&m==0) {
        s = 0;
        m = 0;
    }
    marker(0.3525, -0.4742, 0.0933, 2.5);
}

item("Magnetos") {
    state("CHECK");
